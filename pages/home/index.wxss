page{  
  display: flex;  
  flex-direction: column;
  /* flex-wrap: wrap;   */
  height: 100%;  
}
/* 搜索框样式 */
.search_contain{
  width: 100%;
  height: 150rpx;
  background-color: 	#7B68EE;
  /* background-color: red; */
  display: inline-block;
  /* flex-direction: row;
  justify-content:center; */
  position:relative;
}
.search
{
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items: center;
  width:95%;
  height:80rpx;
  background-color:whitesmoke;
  margin: 5px 2%; 
  border:1rpx solid whitesmoke;
  border-radius: 5px;
  z-index: 2;
  /* padding-top: 10rpx;  */
}
.search .text{
  width: 85%;
}

/* 搜索框提示文字样式 */
.search input
{
  font-size:30rpx;
  font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  padding-left:30rpx; 
}

/* 搜索框小图标 */
.search image
{ 
  width:45rpx;
  height:45rpx;
  padding-right:20rpx;
}
.head{
  /* background-color: red; */
  display: flex;
  /* width: 98%; */
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: left;
  /* margin:2px 1%; */
  /* margin-top:100rpx; */
  z-index: 1;
}

.type{
  width:24%;
  font-size: 20px;
  text-align: center;
  border: 1px solid black;
  border-radius: 6px;
  margin-left: 1px;
  margin-top: 1px;
  padding-top: 5px;
  padding-bottom: 5px;
}
.active-tag .type{
  background: #03e2ff;
  color: #fff;
  border: 1rpx solid #fff;
}

